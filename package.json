{
  "name": "job-application",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose --env-file .env.local up -d && next dev --turbopack",
    "build": "prisma migrate deploy && prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate:prod": "dotenv -e .env -- prisma generate",
    "db:push:prod": "dotenv -e .env -- prisma db push",
    "db:migrate:prod": "dotenv -e .env -- prisma migrate deploy",
    "db:seed:prod": "dotenv -e .env -- npx tsx prisma/seed.ts",
    "db:studio:prod": "dotenv -e .env -- prisma studio",
    "db:generate:dev": "dotenv -e .env.local -- prisma generate",
    "db:push:dev": "dotenv -e .env.local -- prisma db push",
    "db:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "db:seed:dev": "dotenv -e .env.local -- npx tsx prisma/seed.ts",
    "db:studio:dev": "dotenv -e .env.local -- prisma studio",
    "db:reset:dev": "dotenv -e .env.local -- prisma migrate reset"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.16.0",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@types/leaflet": "^1.9.20",
    "@types/three": "^0.179.0",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-auth": "^5.0.0-beta.29",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^5.0.0",
    "three": "^0.179.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "prisma": "^6.14.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
